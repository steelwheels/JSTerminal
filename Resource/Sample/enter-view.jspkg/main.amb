top: VBox {
    enter_button: Button {
   	title: String "SubView"
	pressed: Event() %{
		let ret = enterView("sub") ;
		console.log("message: " + ret.message + ", a  = " + ret.a + ", b = " + ret.b) ;
        %}
    }
    quit_button: Button {
   	title: String "Quit"
	pressed: Event() %{
		leaveView(0) ;
        %}
    }
}

