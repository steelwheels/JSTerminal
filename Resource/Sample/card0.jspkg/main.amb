top: VBox {
    table: Card {
	indexField: TextField {
		text: String Listner(index: top.table.index) %{
			return "index: " + index ;
		%}
	}
	familyName: TextField {
		text: String Listner(record: top.table.contact) %{
			return "family-name: " + record.familyName ;
		%}
	}
    }
    quit_button: Button {
   	title: String "Quit"
	pressed: Event() %{
		leaveView(1) ;
        %}
    }
}

