top: VBox {
    stepper0: Stepper {
	isEnabled:     		Bool	true
	maxValue:      		Float	10.0
	minValue:      		Float	 0.0
	deltaValue:    		Float	 0.1
	currentValue:  		Float	 1.0
        decimalPlaces: 		Int       2
	isIncrementable:	Bool	true
	isDecrementable:	Bool	true
        changed: Event(newval) %{
		console.log("changed: " + newval) ;
        %}
    }
    quit_button: Button {
   	title: String "Quit"
	pressed: Event() %{
		leaveView(1) ;
        %}
    }
}

